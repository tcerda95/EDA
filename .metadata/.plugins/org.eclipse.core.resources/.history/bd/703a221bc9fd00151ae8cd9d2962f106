package tp3;

public class PriorityQueue<T> {
	
	private static class Node<T> {
		Queue<T> queue;
		int priority;
		public Node(Queue<T> q, int p) {
			queue = q;
			priority = p;
		}
	}
	
	private Node<T> first; // contiene la menor prioridad
	
	public void enqueue(T elem, int priority) {
		enqueue(elem, priority, first);
	}
	
