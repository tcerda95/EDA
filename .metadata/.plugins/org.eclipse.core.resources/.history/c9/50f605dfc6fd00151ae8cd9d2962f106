package tp3;

public class Queue<T> {
	private static class Node<T> {
		private T value;
		private Node<T> next;
		
		public Node(T v) {
			value = v;
		}
	}
	
	private Node<T> first;
	private Node<T> last;
	
	public void enqueue(T value) {
		Node<T> n = new Node<T>(value);
		
		if (first == null) {
			first = n;
			last = n;
		}
		else
			last.next = n;
	}
}
